export PATH="./x64:$PATH"
export DECAY=0.95
export RESTART=geometric
export SCALE=250
export BASE=1.5
export SAMPLES=5
export NGL=256
export TIMEOUT=600000

for b in letters-afc; do
    for d in uk words; do
	for ((i=10; i<50; i++)); do
	    ./x64/crossword.exe \
		-time $TIMEOUT -branching $b \
		-file words/$d.txt $i | tee logs/$b/base/stat-$d-$i.txt
	    ./x64/crossword.exe \
		-decay $DECAY -restart $RESTART \
		-restart-scale $SCALE -restart-base $BASE \
		-nogoods true -nogoods-limit $NGL \
		-time $TIMEOUT -branching $b \
		-file words/$d.txt $i | tee logs/$b/nogoods/stat-$d-$i.txt
	    ./x64/crossword.exe -mode time -samples $SAMPLES \
		-time $TIMEOUT -branching $b  \
		-file words/$d.txt $i | tee logs/$b/base/time-$d-$i.txt
	    ./x64/crossword.exe -mode time -samples $SAMPLES \
		-decay $DECAY -restart $RESTART \
		-restart-scale $SCALE -restart-base $BASE \
		-nogoods true -nogoods-limit $NGL \
		-time $TIMEOUT -branching $b \
		-file words/$d.txt $i | tee logs/$b/nogoods/time-$d-$i.txt
	done
    done
done

