export PATH="./x64:$PATH"
export TIMEOUT=60000

#for d in uk words; do
#  for ((i=10; i<50; i++)); do
#    ./x64/crossword.exe \
#	  -time $TIMEOUT -branching letters \
#	  -file words/$d.txt $i | tee base/stat-$d-$i.txt
#  done
#done
for d in uk words; do
  for ((i=10; i<50; i++)); do
    ./x64/crossword.exe -decay 0.99 -restart geometric \
	-restart-scale 2000 -restart-base 2 \
	-time $TIMEOUT -branching letters \
	-file words/$d.txt $i | tee restart/stat-$d-$i.txt
  done
done
