export PATH="./x64:$PATH"
export SAMPLES=1
export THREADS=1
export TIMEOUT=1200000
export DECAY=0.99

for d in uk words; do
  for ((i=10; i<50; i++)); do
    ./x64/crossword.exe -mode time -samples $SAMPLES \
	-time $TIMEOUT -threads $THREADS -branching letters -decay $DECAY \
	-file words/$d.txt $i | tee logs/$THREADS/$DECAY/stat-$d-$i.txt
  done
done
