export PATH="./x64:$PATH"
export DECAY=0.95
export SCALE=250
export RESTART=geometric
export BASE=1.5
export NGL=256
export TIMEOUT=0

# Where none can solve
for b in letters-afc letters-action; do
    for i in 39 45 49; do
	./x64/crossword.exe -decay $DECAY -restart $RESTART \
	    -restart-scale $SCALE -restart-base $BASE \
	    -nogoods true -nogoods-limit $NGL \
	    -time $TIMEOUT -branching $b \
	    -file words/words.txt $i | tee logs/$b/hard/words-nogoods-$i-$TIMEOUT.txt
    done
done
